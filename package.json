    "@dhis2/d2-ui-org-unit-tree": "latest",
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.19.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "d2": "^28.1.4",
    "d2-manifest": "^1.0.0",
    "d2-utilizr": "^0.2.16",
    "dateformat": "^3.0.3",
    "excel4node": "^1.7.2",
    "exceljs": "^1.6.2",
    "file-saver": "^1.3.8",
    "filereader-stream": "^2.0.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.10",
    "material-ui": "0.20.1",
    "moment": "^2.24.0",
    "prettier": "^1.17.1",
    "prop-types": "latest",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-dropzone": "^6.2.4",
    "react-redux": "^5.0.7",
    "react-scripts": "^1.1.4",
    "react-select": "^2.1.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "rm -rf bulk-load.zip && rm -rf build/",
    "build": "react-scripts build",
    "postbuild": "d2-manifest package.json build/manifest.webapp && cd build && zip -r ../bulk-load.zip . && rm asset-manifest.json && rm service-worker.js && cd ..",
    "prettify": "prettier \"{src,public}/**/*.{js,jsx,ts,tsx,json,css}\" --write",
    "test": "node scripts/test.js --env=jsdom",
    "eject": "react-scripts eject"
  },
  "manifest.webapp": {
    "name": "Bulk Load",
    "description": "Bulk importing made easy",
    "version": "2.0.3",
    "developer": {
      "name": "Alexis Rico",
      "url": "https://github.com/SferaDev"
    },
    "icons": {
      "48": "img/icon.png"
    },
    "activities": {
      "dhis": {
        "href": "*"
      }
    }
  },
  "homepage": "."
}
